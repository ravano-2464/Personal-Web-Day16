<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="assets/css/my-testimonials.css" />
    <link rel="icon" href="/assets/icon/dumbways.ico" type="image/x-icon">
    <link
      href="https://cdn.jsdelivr.net/npm/remixicon@3.4.0/fonts/remixicon.css"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
      integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css"
      integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <title>Personal Website - {{titlePage}}</title>
  </head>
  <body>
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
      <div class="container-fluid px-4 py-4">
        <a class="navbar-brand" href="/"><img
            class="logo"
            src="assets/img/dumbways-logo.png"
            alt="dumbways logo"
          /></a>
        <button
          class="navbar-toggler"
          type="button"
          data-bs-toggle="collapse"
          data-bs-target="#navbarNav"
          aria-controls="navbarNav"
          aria-expanded="false"
          aria-label="Toggle navigation"
        >
          <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse ms-5 me-5" id="navbarNav">
          <ul class="navbar-nav grid gap-4">

            {{#if handleLogin}}
              <li class="nav-item">
                <a
                  class="nav-link fs-2 fw-medium"
                  aria-current="page"
                  href="/"
                ><i class="fa fa-home"></i> Home</a>
              </li>
              <li class="nav-item">
                <a class="nav-link fs-2 fw-medium" href="/my-project"><i class="fa fa-desktop"></i> Add My Project</a>
              </li>
              <li class="nav-item">
                <a
                  class="nav-link active fs-2 fw-medium"
                  href="/my-testimonials"
                ><i class="fa fa-star"></i> My Testimonials</a>
              </li>
              <li class="nav-item">
                <a class="nav-link fs-2 fw-medium" href="#">Hallo
                  {{user}}</a>
              </li>

              <li class="nav-item">
                <a class="nav-link fs-2 fw-medium" href="/logout"><i class="fa fa-sign-out"></i> Logout</a>
              </li>
            {{else}}
              <li class="nav-item">
                <a
                  class="nav-link fs-2 fw-medium"
                  aria-current="page"
                  href="/"
                ><i class="fa fa-home"></i> Home</a>
              </li>
              <li class="nav-item">
                <a
                  class="nav-link active fs-2 fw-medium"
                  href="/my-testimonials"
                ><i class="fa fa-star"></i> My Testimonials</a>
              </li>
              <li class="nav-item">
                <a
                  class="nav-link fs-2 fw-medium"
                  href="/register"
                ><i class="fa fa-registered"></i> Registers</a>
              </li>

              <li class="nav-item">
                <a class="nav-link fs-2 fw-medium" href="/login"><i class="fa fa-sign-in"></i> Login</a>
              </li>
            {{/if}}

            {{! <li class="nav-item">
              <a
                class="nav-link fs-2 fw-medium"
                aria-current="page"
                href="/"
              ><i class="fa fa-home"></i> Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link fs-2 fw-medium" href="/my-project">Add My Project</a>
            </li>
            <li class="nav-item">
              <a
                class="nav-link active fs-2 fw-medium"
                href="/my-testimonials"
              >My Testimonials</a>
            </li>
            <li class="nav-item">
              <a class="nav-link fs-2 fw-medium" href="/register"><i class="fa fa-registered"></i> Registers</a>
            </li>

            <li class="nav-item">
              <a class="nav-link fs-2 fw-medium" href="/login">Login</a>
            </li> }}
          </ul>
        </div>
        <a
          class="btn btn-primary px-5 py-3 fs-2 bg-dark rounded-pill border-0 d-none d-lg-block"
          href="/contact"
          role="button"
        ><i class="fa fa-phone"></i> Contact Me</a>
      </div>
    </nav>

    <main>
      <section class="my-testimonials">
        <h1 class="testimonials-heading"><i class="fa fa-star"></i> MY TESTIMONIALS</h1>
        <div class="rating-box">
          <button class="rating-btn rating-btn-el" onclick="allTestimonials()">
            All<i class="ri-star-fill"></i>
          </button>

          <button class="rating-btn" onclick="filterTestimonials(1)">
            1<i class="ri-star-fill"></i>
          </button>

          <button class="rating-btn" onclick="filterTestimonials(2)">
            2<i class="ri-star-fill"></i>
          </button>

          <button class="rating-btn" onclick="filterTestimonials(3)">
            3<i class="ri-star-fill"></i>
          </button>

          <button class="rating-btn" onclick="filterTestimonials(4)">
            4<i class="ri-star-fill"></i>
          </button>

          <button class="rating-btn" onclick="filterTestimonials(5)">
            5<i class="ri-star-fill"></i>
          </button>
        </div>
        <div class="container list-container">
          <div class="testimonials-box"></div>
        </div>
      </section>
    </main>
    <script src="assets/script/my-testimonials.js"></script>

    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js"
      integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+"
      crossorigin="anonymous"
    ></script>
  </body>
</html>